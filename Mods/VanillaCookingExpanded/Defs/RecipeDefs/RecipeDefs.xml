<?xml version="1.0" encoding="utf-8" ?>
<Defs>

					<RecipeDef>
						<defName>VCE_HackleLoSyrup</defName>
						<label>make hackle-lo syrup</label>
						<description>Create hackle-lo syrup from hackle-lo. Hackle-lo syrup is a thin and semi-sweet syrup, known to reduce fatigue in those who consume it.</description>
						<jobString>Making hackle-lo syrup.</jobString>
						<workSpeedStat>CookSpeed</workSpeedStat>
						<effectWorking>Cook</effectWorking>
						<soundWorking>Recipe_CookMeal</soundWorking>
						<workAmount>200</workAmount>
						<recipeUsers>
							<li>VCE_CondimentPrepTable</li>	  
						</recipeUsers>	
						<ingredients>
							<li>
								<filter>
									<thingDefs>
										<li>MorrowRim_RawHackleLo</li>
									</thingDefs>
								</filter>
								<count>10</count>
							</li>
						</ingredients>
						<fixedIngredientFilter>
							<thingDefs>
								<li>MorrowRim_RawHackleLo</li>
							</thingDefs>
						</fixedIngredientFilter>
						<products>
							<VCE_HackleLoSyrup>5</VCE_HackleLoSyrup>
						</products>
						<researchPrerequisite>MorrowRim_AshCuisine</researchPrerequisite>
					</RecipeDef>
			
